            }
            var plantype = $("#type3  option:selected").text();
            if(plantype=="日计划"){
                plantype="01";
            }else if(plantype=='月计划'){
                plantype='02';
            }else{
                plantype='03'
            }
            if($("#plan").html()!="记录您工作计划..." || $("#fileList").html()!="") {
                var reg = "/^\w+$/"
                var hour = $("#hours").val()
                var hours = $("#hourss").val()
                if(!(/^\d+(\.\d+)?$/.test(hour) && (/^\d+(\.\d+)?$/.test(hours)))){
                   alert("请以数字形式填写工作量或计划量");
                   return false;
                }else{
                    var workhour = $("#hours").val();
                }
                var data = {"plantext": plantext, "workhour": workhour, "plantype": plantype,"id":plan_id,"workload":hours};
                $.ajax({
                    url: "/tplan/InsertPlan",

        function table1() {
            function leixing(value,row,index){
                //序号正序排序从1开始
                if (lblx == "月"){
                    return "月";
                }else if(lblx == "季"){
                    return "季";
                }else if(lblx == "年"){
                    return "年";
                }

            };
            function shijian(value,row,index){
                //序号正序排序从1开始
                return lbsj;
            };

            var columns = [
                {
                    field: 'number',
                    title: '排名',
                    //width:'5px' ,
                    align:'center',
                    formatter: function (value,row,index){
                        ++index;
                        if(index ==1){
                            return '<img src="/static/img/icon-test.jpg" style="width:26px;height:23px;">';
                        }else if(index ==2){
                            return  '<img src="/static/img/icon-test-(1).jpg" style="width:26px;height:23px;">';

                        }else if(index ==3){
                            return  '<img src="/static/img/icon-test-(2).jpg" style="width:26px;height:23px;">';
                        }else{
                            return index;
                        }
                        //序号正序排序从1开始
                    }
                },
                {
                    field: 'type',
                    title: '类型',
                    align:'center',
                    formatter:leixing
                }, {
                    field: 'time',
                    title: '时间',
                    align:'center',
                    formatter:shijian
                },{
                    field: 'praisenum',
                    title: '点赞',
                    align:'center',
                    formatter: function (value,row,index){
                        return '<i class="iconfont icon-buoumaotubiao15" style="font-size: 15px;">&nbsp;'+row.praisenum+'</i>';
                        //序号正序排序从1开始
                    }
                }, {
                    field: 'commentnum',
                    title: '评论',
                    align: 'center',
                    width: '140px',
                    formatter: function (value,row,index){
                        return '<i class="iconfont icon-pinglun4" style="font-size: 15px;">&nbsp;'+row.commentnum+'</i>';
                        //序号正序排序从1开始
                    }

                }]
            var partcolumnsxm ={field: 'name',title: '姓名',align:'center', };
            var partcolumnsxz ={field: 'name',title: '小组',align:'center', };
            var partcolumnsbm ={field: 'name',title: '部门',align:'center', };
            var partcolumnsyj ={field: 'realynum',title: '业绩',align:'center', };
            var partcolumnsxl ={field: 'realycient',title: '效率',align:'center', };
            var partcolumnsgzsc ={field: 'realyhours',title: '工作时长',align:'center', };

            if(object =="01"  && types =="01"){
                columns.splice(3,0,partcolumnsxm );
                columns.splice(4,0,partcolumnsyj );
            }else if(object =="01" && types =="02"){
                columns.splice(3,0,partcolumnsxm);
                columns.splice(4,0,partcolumnsxl );
            }else if(object =="01" && types =="03"){
                columns.splice(3,0,partcolumnsxm );
                columns.splice(4,0,partcolumnsgzsc );
            }else if(object =="02" && types =="01"){
                columns.splice(3,0,partcolumnsxz);
                columns.splice(4,0,partcolumnsyj);
            }else if(object =="02" && types =="02"){
                columns.splice(3,0,partcolumnsbm );
                columns.splice(4,0,partcolumnsxl );
            }else if(object =="02" && types =="03"){
                columns.splice(3,0,partcolumnsxz);
                columns.splice(4,0,partcolumnsgzsc );
            }else if(object =="03" && types =="01"){
                columns.splice(3,0,partcolumnsbm );
                columns.splice(4,0,partcolumnsyj );
            }else if(object =="03" && types =="02"){
                columns.splice(3,0,partcolumnsbm);
                columns.splice(4,0,partcolumnsxl );
            }else if(object =="03" && types =="03"){
                columns.splice(3,0,partcolumnsbm );
                columns.splice(4,0,partcolumnsgzsc );
            };


            $('#table1').bootstrapTable({
                /*showToggle:!0,
                showColumns:!0,*/
                iconSize:"outline",
                toolbar:"#exampleTableEventsToolbar",
                /* icons:{refresh:"glyphicon-repeat",toggle:"glyphicon-list-alt",columns:"glyphicon-list"},*/
                url: "<%=request.getContextPath()%>/api/getDataList.do", // 获取表格数据的url
                /*showRefresh:true,*/
                cache: false, // 设置为 false 禁用 AJAX 数据缓存， 默认为true
                striped: true,  //表格显示条纹，默认为false
                pagination: true, // 在表格底部显示分页组件，默认false
                pageList: [5,10, 15], // 设置页面可以显示的数据条数
                pageSize: 5, // 页面数据条数
                pageNumber: 1, // 首页页码
                onClickRow: function (row, $element,field) {
                    if(object == "01"){
                        page("/manage?userid="+row.userid+"&name="+row.name+"&starttime="+starttimeclick+"&endtime="+endtimeclick,"业绩管理");
                    }else if(object == "02"){
                        page("/manage?zdeparid="+row.zdeparid+"&name="+row.name+"&starttime="+starttimeclick+"&endtime="+endtimeclick,"业绩管理");
                    }else if(object == "03"){
                        page("/manage?bdeparid="+row.bdeparid+"&name="+row.name+"&starttime="+starttimeclick+"&endtime="+endtimeclick,"业绩管理");
                    }

                },
                columns:columns,
                data:datatableyj
            })
            $('#table1').bootstrapTable(
                "refreshOptions",
                {
                    columns : columns,
                }
            );
}
        
        

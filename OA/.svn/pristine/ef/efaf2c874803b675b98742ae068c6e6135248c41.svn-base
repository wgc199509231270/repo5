<script>
    $(function() {
        queryUser();
        $("#big").show();
        $("#example").hide();
        $("#paimingtable").show();
        now();
        before();
        tabledata();
    });
    var userid;
    var astarttime ;
    var aendtime ;
    //格式化现在日期
    function now() {
        // 给input  date设置默认值
        var now = new Date();
        //格式化日，如果小于9，前面补0
        var day = "01";
        //格式化月，如果小于9，前面补0
        var month = ("0" + (now.getMonth() + 2)).slice(-2);
        var year =now.getFullYear();
        if(month >12){ month ="01"; year = now.getFullYear()+1};
        //拼装完整日期格式
        var today = year+"-"+(month)+"-"+(day) ;
        //完成赋值
        $('#Endtime0').val(today);
        aendtime =$('#Endtime0').val();
    };
    //格式化15天前的日期
    function before() {
        //十五天毫秒值：1209600000
        var date = new Date();
        var times = date.getTime();
        var now = new Date(times);
        //格式化日，如果小于9，前面补0
        var day = "01";
        //格式化月，如果小于9，前面补0
        var month = ("0" + (now.getMonth() + 1)).slice(-2);
        //拼装完整日期格式
        var today = now.getFullYear()+"-"+(month)+"-"+(day) ;
        //完成赋值
        $('#Begtime0').val(today);
        astarttime=$('#Begtime0').val();
    };
    var datatable;
    //表格接口
    function tabledata(){
        var options=$("#options").val();
        var status=$("#status").val();
        var Begtime0=$("#Begtime0").val();
        var Endtime0=$("#Endtime0").val();
        var text=$("#text").val();
        var status2=status;
        if(status=="02"){
            status="01";
        }
        var  data = {"regstarttime":Begtime0,"regendtime":Endtime0,"alluser":options,"status":status,"text":text};
        $.ajax({
            url:"/tactivity/queryTactivity",
            data: data,
            async:false,
            dataType:"json",
            type:"post",
            success:function (data) {
                if(status2=="02"){
                    var datas=[];
                    var newdate=getNewDate();
                   for(var i=0;i<data.length;i++){
                       if(data[i].regstarttime<=newdate && data[i].regendtime>newdate){
                           datas.push(data[i]);
                       }
                   }
                   datatable=datas;
                }else{
                    datatable=data;
                }

            }
        });
    }

    //表格代码
    function table(){
        $('#table1').bootstrapTable({
            showToggle:!0,
            showColumns:!0,
            iconSize:"outline",
            toolbar:"#exampleTableEventsToolbar",
            icons:{refresh:"glyphicon-repeat",toggle:"glyphicon-list-alt",columns:"glyphicon-list"},
            url: "<%=request.getContextPath()%>/api/getDataList.do", // 获取表格数据的url
            showRefresh:true,
            cache: false, // 设置为 false 禁用 AJAX 数据缓存， 默认为true
            striped: true,  //表格显示条纹，默认为false
            pagination: true, // 在表格底部显示分页组件，默认false
            pageList: [5,10, 15], // 设置页面可以显示的数据条数
            pageSize: 5, // 页面数据条数
            pageNumber: 1, // 首页页码
            onClickRow: function (row, $element) {
            },
            onClickCell:function(field, value, row, $element,event){
                if(field!="operate"){
                    page('/activityMess?id='+row.id,'活动详情');
                }
            },
            columns: [
                {
                    field: 'number',
                    title: '序号',
                    width:'4%' ,
                    align:'center',
                    formatter: function (value,row,index){
                        return index+1; //序号正序排序从1开始
                    }
                },
                {
                    field: 'activname',
                    title: '活动名称',
                    width:'7%' ,
                    align:'center',
                },
                {
                    field: 'address',
                    width:'7%' ,
                    title: '活动地址',
                    align:'center',
                },{
                    field: 'starttime',
                    width:'10%' ,
                    title: '活动时间',
                    align:'center',
                },{
                    field: 'regstarttime',
                    width:'10%' ,
                    title: '报名开始时间',
                    align:'center',
                },{
                    field: 'regendtime',
                    width:'10%' ,
                    title: '报名结束时间',
                    align:'center',

                },{
                    field: 'status',
                    width:'10%' ,
                    title: '活动状态',
                    align:'center',
                    formatter: function (value,row,index){
                        var newstime=getNewDate();
                        if(row.status =="01"){
                            if(row.regstarttime>=newstime && row.regendtime<newstime){
                                return "报名中";
                            }else{
                                return "未开始";
                            }
                        }else if(row.status =="02"){
                            return "进行中";
                        }else if(row.status =="03"){
                            return "已结束";
                        }else{
                            return "-";
                        }
                    }

                }, {
                    field: 'operate',
                    title: '操作',
                    width:'10%' ,
                    align: 'center',
                    formatter: operate
                }
            ] ,
            data:datatable
        });
    }
    //表格操作权限
    function operate(value,row,index){
        var ret="";
        if(row.promulgatorid===userid){
            ret+="<a href='javascript:void(0);' id='"+row.id+"' class='czbtn' onclick='compile(this.id)' style='color:#0099CC;'>编辑</a> " +
                "<a href='javascript:void(0);' id='"+row.id+"' class='czbtn' onclick='deletedata(this.id)' style='color:#0099CC;'>删除</a> " +
                "<a href='javascript:void(0);' id='"+row.id+"' class='czbtn' onclick='gotomess(this.id)' style='color:#0099CC;'>报名信息</a> ";
        }
        if(ret==""){
            return "<a href='javascript:void(0);' id='"+row.id+"' class='czbtn' onclick='gotopage(this.id)' style='color:#0099CC;'>详情</a>"
        }else{
            return ret;
        }
    }
    function compile(id) {
        $.ajax({
            url:"/tactivity/queryTactivity",
            data: {"id":id},
            async:false,
            dataType:"json",
            type:"post",
            success:function (data) {
                $("#activname").html(data[0].activname);
                $("#theme").html(data[0].theme);
                $("#starttime").val(data[0].starttime);
                $("#endtime").val(data[0].endtime);
                $("#regstarttime").val(data[0].regstarttime);
                $("#regendtime").val(data[0].regendtime);
                $("#address").html(data[0].address);
                $("#content").html(data[0].content);
                $("#requires").html(data[0].requires);
                $("#remark").html(data[0].remark);
                $("#getId").val(data[0].id);
                issue(2);
            }
        });
    }
    function deletedata(id) {
        var msg = "是否删除？";
        if (confirm(msg)==true){
            $.ajax({
                url:"/tactivity/delTactivity",
                data: {"id":id},
                async:false,
                dataType:"json",
                type:"post",
                success:function (data) {
                    chongzhi();
                }
            });
        }
    }
    function gotomess(id) {
        page('/activityRegMess?id='+id,'报名信息');
    }
    function gotopage(id) {
        page('/activityMess?id='+id,'活动详情');
    }
    //选项卡设置
    function page(url, title) {
        var nav = $(window.top.document).find('.J_menuTabs .page-tabs-content ');
        $(window.top.document).find('.J_menuTabs .page-tabs-content ').find(".J_menuTab.active").removeClass("active");
        $(window.top.document).find('.J_mainContent').find("iframe").hide();
        var iframe = '<iframe class="J_iframe" name="iframe10000" width="100%" height="100%" src="' + url + '" frameborder="0" data-id="' + url
            + '" seamless="" style="display: inline;"></iframe>';
        $(window.top.document).find('.J_menuTabs .page-tabs-content ').append(
            ' <a href="javascript:;" class="J_menuTab active" data-id="'+url+'">' + title + ' <i class="iconfont icon-guanbi-copy" style="font-size: 28px;color:#D8D9DA;float: right;margin-left: -10px;width: 20px;margin-top: -5px"></i></a>');
        $(window.top.document).find('.J_mainContent').append(iframe);
    }
    function chaxun(){
        tabledata();
        $('#table1').bootstrapTable('load',datatable);
    }

    function chongzhi(){
        now();
        before();
       $("#options").val("01");
       $("#status").val("00");
       $("#text").val("");
        chaxun();
    }
    function issue(i){
        if(i===1){
            $("#issues").html("活动发布");
        }else if(i===2){
            $("#issues").html("活动修改");
        }
        $("#modal-form").modal('show');
        $(".modal-dialog").show();
    }
    /*活动发布*/
    function affirm(){
        var activname=$("#activname").html();
        var theme=$("#theme").html();
        var starttime=$("#starttime").val();
        var endtime=$("#endtime").val();
        var regstarttime=$("#regstarttime").val();
        var regendtime=$("#regendtime").val();
        var address=$("#address").html();
        var content=$("#content").html();
        var requires=$("#requires").html();
        var remark=$("#remark").html();
        var data={"activname":activname,"theme":theme,"starttime":starttime,"endtime":endtime,"regstarttime":regstarttime,"regendtime":regendtime,"address":address,"content":content,"requires":requires,"remark":remark};
        if($("#issues").html()==="活动发布"){
            $.ajax({
                url:"/tactivity/insertTactivity",
                data: data,
                async:false,
                dataType:"json",
                type:"post",
                success:function (data) {
                    console.log(data);
                }
            });
        }else if($("#issues").html()==="活动修改"){
            data.id=$("#getId").val();
            $.ajax({
                url:"/tactivity/updateTactivity",
                data: data,
                async:false,
                dataType:"json",
                type:"post",
                success:function (data) {
                    console.log(data);
                }
            });
        }
        cancel();
        chongzhi();
    }
    function cancel(){
        $("#modal-form").modal('hide');
        $("#activname").html("");
        $("#theme").html("");
        $("#starttime").val("");
        $("#endtime").val("");
        $("#regstarttime").val("");
        $("#regendtime").val("");
        $("#address").html("");
        $("#content").html("");
        $("#requires").html("");
        $("#remark").html("");
    }
    $(function(){
        <!-- jeDate 插件 -->
        jeDate("#Begtime0",{
            format:"YYYY-MM-DD",
            isTime:false,
            minDate:"2014-09-19"
        });

        jeDate("#Endtime0",{
            format:"YYYY-MM-DD",
            isTime:false,
            minDate:"2014-09-19"
        });
        jeDate("#regstarttime",{
            format:"YYYY-MM-DD hh:mm:ss",
            isTime:false,
            minDate:"2014-09-19 00:00:00"
        });
        jeDate("#regendtime",{
            format:"YYYY-MM-DD hh:mm:ss",
            isTime:false,
            minDate:"2014-09-19 00:00:00"
        });
        jeDate("#starttime",{
            format:"YYYY-MM-DD hh:mm:ss",
            isTime:false,
            minDate:"2014-09-19 00:00:00"
        });
        jeDate("#endtime",{
            format:"YYYY-MM-DD hh:mm:ss",
            isTime:false,
            minDate:"2014-09-19 00:00:00"
        });
    });
    function setStatus() {
        if($("#options").val()==="01"){
            $("#status").find("option").remove();
            $("#status").append('<option value="00">全部</option>\n' +
                '                <option value="01">未开始</option>\n' +
                '                <option value="02">报名中</option>\n' +
                '                <option value="03">已结束</option>')
        }else if($("#options").val()==="02"){
            $("#status").find("option").remove();
            $("#status").append('<option value="00">全部</option>\n' +
                '                <option value="01">已报名</option>\n' +
                '                <option value="02">已结束</option>')
        }
    }
    function queryUser() {
        $.ajax({
            url: "/tuser/queryIdTuser",
            type: "post",
            dataType: 'json',
            success: function (data) {
                userid = data.id;
                table();//表格
            }
        })
    }
    function getNewDate() {
        var date=new Date();
        var year=date.getFullYear();
        var month=date.getMonth()+1;
        var day=date.getDate();
        var hours=date.getHours();
        var minu=date.getMinutes();
        var seconds=date.getSeconds();
        if(month<10){
            month="0"+month;
        }
        if(day<10){
            day="0"+day;
        }
        if(hours<10){
            hours="0"+hours;
        }
        if(minu<10){
            minu="0"+minu;
        }
        if(seconds<10){
            seconds="0"+seconds;
        }
        console.log(year+"-"+month+"-"+day+" "+hours+":"+minu+":"+seconds);
        return year+"-"+month+"-"+day+" "+hours+":"+minu+":"+seconds;
    }
</script>
<body>
<div id="big" class="col-sm-12" style="display: none">
    <div class="example-wrap" style="margin-top: 25px;">
        <div id="toolbar1"
             style="width: 100%; overflow: hidden; margin-bottom: 14px; float: left;">
            <div class="myText-content" style="float: left;width: 11%;cursor:pointer;">
                <label class="myLabel-content">选项:&nbsp;&nbsp;&nbsp;</label>
                <select onchange="setStatus()"
                        name="select" id="options"
                        style="float: left; width: 80px; height: 33px; line-height: 35px; border: solid 1px #D9D9D9;">
                    <option value="01">全部活动</option>
                    <option value="02">我的活动</option>
                </select>
            </div>


            <div class="myText-content" style="float: left;width: 10%;cursor:pointer;">
                <label class="myLabel-content">状态:&nbsp;&nbsp;&nbsp;</label> <select
                    name="select" id="status" style="float: left; width: 70px; height: 33px; line-height: 35px; border: solid 1px #D9D9D9;">
                <option value="00">全部</option>
                <option value="01">未开始</option>
                <option value="02">报名中</option>
                <option value="03">已结束</option>
            </select>
            </div>

            <div class="myText-content" style="float: left;width: 13%;">
                <label class="myLabel-content">活动:&nbsp;&nbsp;&nbsp;</label>
                <input id="text" style="width: 104px;border: solid 1px #D9D9D9;" value="" placeholder="请输入关键字"/>
            </div>
